{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"module": "es2015",
		"outDir": "./dist",
		"baseUrl": "",
		"emitDecoratorMetadata": false,
		"strict": true
	},
	"exclude": [
		"src/test.entry.ts",
		"src/**/*.spec.ts",
		"node_modules"
	],
	"angularCompilerOptions": {
		"strictMetadataEmit": true,
		"skipMetadataEmit": true,
		"genDir": "./dist",
		"entryModule": "demo/demo.module#DemoModule",
		"trace": true
	}
}