{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "es2020",
    "moduleResolution": "node",
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "es2015",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		],
		"paths": {
			"ezy-chart": [
				"dist/ezy-chart"
			],
			"ezy-chart/*": [
				"dist/ezy-chart/*"
			]
		}
	}
}