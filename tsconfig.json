{
	"compilerOptions": {
		"target": "ES5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noUnusedLocals": true,
		"lib": [
			"es2015",
			"dom"
		],
		"strict": false,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules"
	]
}